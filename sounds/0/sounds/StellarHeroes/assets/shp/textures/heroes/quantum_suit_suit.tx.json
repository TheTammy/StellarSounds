{
  "texture": {
    "in": "shp:quantum_suit_nomask",
    "transform": [
      {
        "operation": "XOR",
        "supply": "shp:xor/quantum_suit/<TIMER>"
      }
    ],
    "out": "shp:generated/quantum_suit__suit<TIMER>"
  },
  "getters": {
    "TIMER": {
      "key": "Math.ceil(43 * entity.getInterpolatedData('shp:dyn/suit_timer')) | 0",
      "values": {
        "min": 0,
        "max": 43
      }
    }
  },
  "isDynamic": true
}
